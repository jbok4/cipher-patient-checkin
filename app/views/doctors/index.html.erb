<div>
  <h2>Our Exceptional Team of Doctors</h2>
</div>

<div class="row">
<% @doctors.each do |doctor| %>
  <div class="col-md-3 col-sm-4 col-xs-6">
    <div class="thumbnail">
      <img src="<%= doctor.image %>">
      <div class="caption">
        <h3><%= doctor.name %></h3>
        <p class="btn btn-default"><%= link_to "Select", doctor %></p>
      </div>
    </div>
  </div>
  <% end %>
</div>




